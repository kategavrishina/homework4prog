sym = "0123456789.,?!:;()[]-_|/\"'«»*{}<>@#$%^&№"
s = []
with open("new 1.txt", 'r', encoding='utf-8') as f:
    for line in f:
        line = line.strip()
        line = line.lower()
        words = line.split()
        line = line.replace("  ", " ")
        for word in words:
            s.append(word.strip(sym))
print (s)
    
